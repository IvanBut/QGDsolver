FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    rotor
    {
        type            calculated;//zeroGradient;
        value 		$internalField;
    }

    freestream
    {
	type            calculated;//zeroGradient;
        value 		$internalField;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}

