FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type		qhdFlux;
    }

    outlet
    {
        type            qhdFlux;//zeroGradient;
    }

    upperWall
    {
        type            qhdFlux;//zeroGradient;
    }

    lowerWall
    {
        type            qhdFlux;//zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

