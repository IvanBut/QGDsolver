FoamFile
{
    version     4.1;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoQGDThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       28.6; //this could be any value
    }

    equationOfState
    {
        rho    1000;
    }
    thermodynamics
    {
        Hf              0;
        Cp              4300;
    }

    transport
    {
        mu   0.01;
        Pr   1;
        beta 2.0e-4;
    }
}

QGD
{
    pRefCell    0;
    pRefValue   0;
    implicitDiffusion true;
    QGDCoeffs constTau;
    constTauDict
    {
       Tau  0.001;
    }
}
