FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    sideLeft
    {
          type   slip;
    }
    sideRight
    {
        type    fixedValue;
        value   uniform (-124.82414 0 0);
    }
    sideWall
    {
        type            slip;
    }
}


