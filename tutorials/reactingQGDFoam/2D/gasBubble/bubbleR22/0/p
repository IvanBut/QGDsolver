FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    sideLeft
    {
        type      slip;
    }
    sideRight
    {
        type    fixedValue;
        value   uniform 156983.9256;
    }
    sideWall
    {
        type            slip;
    }
}

